# Install native apps
# Usage: `brew bundle Caskfile`

install caskroom/cask/brew-cask
tap caskroom/versions

cask install google-chrome 2> /dev/null
cask install macvim 2> /dev/null
cask install sublime-text 2> /dev/null
cask install the-unarchiver 2> /dev/null
cask install virtualbox 2> /dev/null
cask install alfred 2> /dev/null
cask install bettertouchtool 2> /dev/null
cask install clipmenu 2> /dev/null
cask install flux 2> /dev/null
cask install xquarts 2> /dev/null
cask install rstudio 2> /dev/null
cask install firefox 2> /dev/null
cask install skype 2> /dev/null
cask install filezilla 2> /dev/null
